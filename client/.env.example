# Appwrite Configuration
# Copy this file to .env.local and fill in your actual values

# Appwrite endpoint URL
NEXT_PUBLIC_APPWRITE_ENDPOINT=https://appwrite.warricksmith.com/v1

# Appwrite project ID (replace 'racedaytest111' hard-coded references)
NEXT_PUBLIC_APPWRITE_PROJECT_ID=your_project_id_here

# Appwrite API key
APPWRITE_API_KEY=your_api_key_here

# Environment (development, production)
NODE_ENV=development

# Client-side logging level (DEBUG, INFO, WARN, ERROR, SILENT)
# DEBUG: All logging (development)
# INFO: Info, warnings, and errors (staging)
# WARN: Warnings and errors only
# ERROR: Critical errors only (production)
# SILENT: No logging
NEXT_PUBLIC_LOG_LEVEL=ERROR

